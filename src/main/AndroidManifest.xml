<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.philj.jameslist">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateItemActivity"
            android:label="@string/create_item"></activity>
        <activity
            android:name=".Item_List_Setup"
            android:label="@string/title_activity_item__list__setup"></activity>
        <activity
            android:name=".CreateStoreActivity"
            android:label="@string/create_store"></activity>
        <activity
            android:name=".CreateList"
            android:label="Setup List"></activity>
    </application>

</manifest>
